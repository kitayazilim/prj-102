---
layout: layouts/base.njk
tags: kurumsal

kurumsal:
    -
        langkey: hakkimizda
        url: /hakkimizda
    -
        langkey: vizyon
        url: /vizyon_misyon
    -
        langkey: kalite
        url: /kalite_politikamiz
    -
        langkey: cevre
        url: /cevre
   
---

<div class="container" style="padding-top: 30px;">
    <div class="row">
        <div class="col-md-4" style="padding: 20px 20px;">
            <h4 style="text-align: center; margin-bottom: 20px;" class="lang" key="kurumsal"></h4>
            <ul class="kurumsalmenu" style="list-style: none; padding-left: 30px;">
                {%- for item in kurumsal -%}
                    {# class="menuactive" #}
                    <li>
                        <a href="{{item.url}}" class="lang" key="{{item.langkey}}"></a>
                    </li>
                {%- endfor -%}
            </ul>
        </div>
        <div class="col-md-8" style="padding: 20px 20px;">
            {{content | safe}}
        </div>
    </div>
</div>