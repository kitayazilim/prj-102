---
layout: layouts/base.njk
tags: ik

kaynaklar:
    -
        langkey: basvuru
        url: /basvuru
    -
        langkey: sosyal
        url: /sosyal_sorumluluk
    -
        langkey: insan
        url: /insan_kaynaklari
    -
        langkey: kvkk
        url: /kvkk
   
---

<div class="container" style="padding-top: 30px;">
    <div class="row">
        <div class="col-md-4" style="padding: 20px 20px;">
            <h4 style="text-align: center; margin-bottom: 20px;" class="lang" key="insan"></h4>
            <ul class="kurumsalmenu" style="list-style: none; padding-left: 30px;">
                {%- for item in kaynaklar -%}
                    {# class="menuactive" #}
                    <li>
                        <a href="{{item.url}}" class="lang" key="{{item.langkey}}"></a>
                    </li>
                {%- endfor -%}
            </ul>
        </div>
        <div class="col-md-8">
            {{content | safe}}
        </div>
    </div>
</div>